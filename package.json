{
  "name": "su-island",
  "version": "0.0.5",
  "description": "一款SSG生成框架, 用于快速搭建文档中心, 类似 vitePress",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@7.9.2",
  "scripts": {
    "pnpm-install": "pnpm install",
    "prepare": "husky install",
    "dev": "tsup --watch",
    "build": "tsup && pnpm build:deps",
    "build:deps": "tsx ./scripts/prebundle.ts",
    "start": "cd build && serve .",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.js,.jsx --quiet ./",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "vitest run",
    "test:init": "vitest --ui",
    "test:e2e": "playwright test",
    "prepare:e2e": "tsx scripts/prepare-e2e.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "tsx ./scripts/release.ts"
  },
  "bin": {
    "su-island": "./bin/island.js"
  },
  "keywords": [
    "vite",
    "ssg"
  ],
  "author": "suzhenghui",
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts}": [
      "eslint --fix"
    ]
  },
  "files": [
    "dist",
    "bin",
    "vendors",
    "README.md",
    "template.html",
    "src/runtime",
    "src/theme-default",
    "src/shared",
    "runtime.d.ts",
    "theme.d.ts"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@playwright/test": "1.26.1",
    "@types/babel__core": "^7.20.0",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@types/babel__traverse": "^7.18.3",
    "@types/fs-extra": "^9.0.13",
    "@types/hast": "^2.3.4",
    "@types/lodash-es": "^4.17.12",
    "@types/mdast": "^3.0.10",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/resolve": "^1.20.2",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitest/ui": "^0.34.6",
    "chalk": "^5.2.0",
    "commitlint": "^17.2.0",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "execa": "5.1.1",
    "husky": "=8.0.3",
    "lint-staged": "=13.2.1",
    "minimist": "^1.2.8",
    "prettier": "^2.7.1",
    "rollup": "^2.78.1",
    "semver": "^7.3.8",
    "tsup": "^6.5.0",
    "tsx": "^3.12.1",
    "typescript": "^4.8.4",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/helper-plugin-utils": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/traverse": "^7.20.13",
    "@iconify-json/carbon": "^1.1.13",
    "@loadable/component": "^5.15.2",
    "@mdx-js/rollup": "2",
    "@mdx-js/mdx": "^2.1.5",
    "@vitejs/plugin-react": "^2.2.0",
    "acorn": "^8.8.1",
    "cac": "^6.7.14",
    "sass": "^1.57.1",
    "serve": "^14.2.3",
    "compression": "^1.7.4",
    "esbuild": "^0.17.8",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "github-slugger": "^2.0.0",
    "hast-util-from-html": "^1.0.0",
    "lodash-es": "^4.17.21",
    "mdast-util-mdxjs-esm": "^1.3.0",
    "ora": "^6.1.2",
    "polka": "^0.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "1.3.0",
    "react-router-dom": "6.4.3",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "rehype-stringify": "^9.0.3",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx-frontmatter": "^2.1.1",
    "remark-mdx": "^2.1.5",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.2",
    "resolve": "^1.22.1",
    "shiki": "^0.11.1",
    "sirv": "^2.0.2",
    "unocss": "^0.48.0",
    "vite": "3.1.4"
  }
}